// Vitest Snapshot v1, https://vitest.dev/guide/snapshot.html

exports[`prettier-plugin-eta-ejs > formats ejs fixture deterministically 1`] = `
"<section>
<% if (locals.posts?.length) { %>
<%# post list %>
  <ul>
<% posts.forEach((post) => { %>
      <li><%= post.title %></li>
<% }) %>
  </ul>
<% } else { %>
  <p>No posts</p>
<% } %>
</section>
"
`;

exports[`prettier-plugin-eta-ejs > formats eta fixture deterministically 1`] = `
"<main>
<% if (it.user) { %>
  <h1>Hello,<%= it.user.name %></h1>

<%~ it.render('partials/stats', { user: it.user }) %>
<% } else { %>
  <p>Please sign in.</p>
<% } %>
</main>
"
`;
